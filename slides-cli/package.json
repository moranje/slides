{
  "name": "slides-cli",
  "description": "\"CLI tool for doing slideshow stuff\"",
  "version": "0.0.0",
  "author": "Martien Oranje @moranje",
  "bin": {
    "slides": "./bin/run"
  },
  "bugs": "https://github.com/moranje/slides/issues",
  "dependencies": {
    "@oclif/command": "1.5.11",
    "@oclif/config": "1.12.9",
    "@oclif/plugin-help": "2.1.6",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.21.2",
    "@oclif/tslint": "3.1.1",
    "@types/node": "10.12.29",
    "@types/shelljs": "0.8.3",
    "globby": "8.0.2",
    "shelljs": "0.8.3",
    "ts-node": "8.0.2",
    "tslint": "5.13.1",
    "typescript": "3.3.3333"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/moranje/slides",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "slides",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "moranje/slides",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
